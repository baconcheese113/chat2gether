type User {
  id: ID! @unique @id
  ip: String
  fingerprint: String
  gender: GenderType!
  lookingFor: [GenderType!]!
  age: Int
  minAge: Int
  maxAge: Int
  lastActive: DateTime!
  isHost: Boolean!
  isConnected: Boolean!
  visited: [User!]! @relation(name: "VisitedUsers", onDelete: SET_NULL)
  updatedAt: DateTime! @updatedAt
  createdAt: DateTime! @createdAt
}

type Feedback {
  id: ID! @unique @id
  text: String!
  updatedAt: DateTime! @updatedAt
  createdAt: DateTime! @createdAt
}

enum GenderType {
  MALE,
  FEMALE,
  M2F,
  F2M
}